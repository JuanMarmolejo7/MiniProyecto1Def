<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.escriturarapida.controller.StatsController">
   <children>
      <ImageView fitHeight="625.0" fitWidth="784.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/La%20tiraste%20malarda.png" />
         </image>
      </ImageView>
      <Label fx:id="levelLabel" layoutX="41.0" layoutY="53.0" prefHeight="55.0" prefWidth="551.0" text="Nivel label" textFill="WHITE">
         <font>
            <Font name="VT323 Regular" size="43.0" />
         </font>
      </Label>
      <Label fx:id="succesLabel" layoutX="41.0" layoutY="147.0" prefHeight="64.0" prefWidth="564.0" text="Bien hechas label" textFill="WHITE">
         <font>
            <Font name="VT323 Regular" size="32.0" />
         </font>
      </Label>
      <Label fx:id="failLabel" layoutX="41.0" layoutY="257.0" prefHeight="55.0" prefWidth="564.0" text="Malas label" textFill="WHITE">
         <font>
            <Font name="VT323 Regular" size="32.0" />
         </font>
      </Label>
      <ImageView id="RegresarFlecha" fx:id="RegresarFlecha1" fitHeight="135.0" fitWidth="185.0" layoutX="22.0" layoutY="407.0" onMouseClicked="#onReturnClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/FlechaBack.png" />
         </image>
      </ImageView>
      <Button fx:id="menuButton" layoutX="416.0" layoutY="448.0" mnemonicParsing="false" onAction="#onMenuClick" prefHeight="54.0" prefWidth="88.0" styleClass="btn-menu" stylesheets="CSS/StyleGame.css" text="Menu">
         <font>
            <Font name="VT323 Regular" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
